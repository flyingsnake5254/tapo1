<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:tag="layout/fragment_subg_led_not_blinking_help_0" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
