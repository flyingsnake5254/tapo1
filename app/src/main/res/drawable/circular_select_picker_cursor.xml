<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/cursorStroke">
        <shape android:shape="oval">
            <padding android:left="20dp" android:top="20dp" android:right="20dp" android:bottom="20dp"/>
            <stroke android:width="20dp" android:color="@color/transparent"/>
            <size android:height="100dp" android:width="100dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#f8f8f8"/>
        </shape>
    </item>
    <item android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#f8f8f8"/>
        </shape>
    </item>
    <item android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#f8f8f8"/>
        </shape>
    </item>
    <item android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="#f8f8f8"/>
        </shape>
    </item>
    <item android:id="@+id/fillShape" android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>
