<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" android:useLevel="false">
            <solid android:color="@color/transparent"/>
            <size android:height="18dp" android:width="18dp"/>
        </shape>
    </item>
    <item android:left="6dp" android:top="6dp" android:right="6dp" android:bottom="6dp">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/common_orange"/>
            <size android:height="6dp" android:width="6dp"/>
        </shape>
    </item>
</layer-list>
