<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:gravity="center" android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="rectangle">
            <size android:height="@dimen/switch_compat_track_height" android:width="@dimen/switch_compat_width"/>
            <corners android:radius="7dp"/>
            <solid android:color="@color/common_blue_button_bg_normal"/>
        </shape>
    </item>
</layer-list>
